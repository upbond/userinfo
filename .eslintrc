{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module",
        "allowImportExportEverywhere": true,
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "spread" : true,
            "restParams" : true
        }
    },

    "env" : {
        "browser" : true,
        "node" : true,
        "es6" : true
    }
}